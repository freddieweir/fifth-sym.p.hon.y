# Fifth Symphony Environment Configuration
# Copy this file to .env and configure with your actual values

# Project Configuration
PROJECT_NAME="Fifth Symphony"

# User Paths (update with your actual username/paths)
USER_PATH="/Users/yourusername"

# External Volume Paths (update with your actual external storage)
EXTERNAL_VOLUME="/Volumes/yourvolume"

# Communication Paths (update with your actual communication directory)
COMMUNICATION_PATH="/Volumes/yourvolume/path/to/communication"

# Symlink Target Paths
# Update these with your actual external script locations
HUMAN_LOG_SCRIPT_PATH="/Volumes/yourvolume/path/to/claude_communication/human_log.sh"

# Optional: Override default domains in templates
# DOMAIN="yourdomain.com"

# Add additional environment variables as needed for your templates